<mvc:View controllerName="com.test.candlegraph.controller.main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
	xmlns:graph="com.test.candlegraph.graph">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<graph:CandleGraph id="CandleGraph1" title="Candle Graph" XAxisLabel="Year" YAxisLabel="Values" 
							showHorizontalDashedLines="true"  showVerticalDashedLines="true"
							candleColor="green" circleMinColor="#FF5733" circleMaxColor="#777777" lineColor="Blue" horizontalDashedLinesColor="Pink" verticalDashedLinesColor="orange"
							lineClick="onLineClicked" candleClick="onCandleClicked" showLegend="true"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>